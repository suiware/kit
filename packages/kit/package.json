{
  "name": "@suiware/kit",
  "version": "0.0.2",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    "./main.css": {
      "import": "./dist/main.css",
      "require": "./dist/main.css"
    },
    "./SuiProvider": {
      "types": "./dist/providers/SuiProvider.d.ts",
      "import": "./dist/providers/SuiProvider.mjs",
      "require": "./dist/providers/SuiProvider.js"
    },
    "./Faucet": {
      "types": "./dist/components/Faucet.d.ts",
      "import": "./dist/components/Faucet.mjs",
      "require": "./dist/components/Faucet.js"
    },
    "./useFaucet": {
      "types": "./dist/hooks/useFaucet.d.ts",
      "import": "./dist/hooks/useFaucet.mjs",
      "require": "./dist/hooks/useFaucet.js"
    },
    "./Balance": {
      "types": "./dist/components/Balance.d.ts",
      "import": "./dist/components/Balance.mjs",
      "require": "./dist/components/Balance.js"
    },
    "./useBalance": {
      "types": "./dist/hooks/useBalance.d.ts",
      "import": "./dist/hooks/useBalance.mjs",
      "require": "./dist/hooks/useBalance.js"
    },
    "./NetworkType": {
      "types": "./dist/components/NetworkType.d.ts",
      "import": "./dist/components/NetworkType.mjs",
      "require": "./dist/components/NetworkType.js"
    },
    "./useNetworkType": {
      "types": "./dist/hooks/useNetworkType.d.ts",
      "import": "./dist/hooks/useNetworkType.mjs",
      "require": "./dist/hooks/useNetworkType.js"
    }
  },
  "files": [
    "dist",
    "main.css"
  ],
  "scripts": {
    "build": "tsup && npm run styles",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "styles": "tailwindcss -c ./tailwind.config.js -i ./src/styles/main.css -o ./dist/main.css --minify"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "postcss": "^8.4.49",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.3.5",
    "typescript": "5.7.2",
    "@mysten/dapp-kit": "^0.14.44",
    "@mysten/sui": "^1.18.0",
    "@mysten/wallet-standard": "^0.13.20",
    "@tanstack/react-query": "^5.62.15",
    "react": "^19.0.0"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.1.6",
    "bignumber.js": "^9.1.2",
    "lucide-react": "^0.469.0"
  },
  "peerDependencies": {
    "@mysten/dapp-kit": "^0.14.44",
    "@mysten/sui": "^1.18.0",
    "@mysten/wallet-standard": "^0.13.20",
    "@tanstack/react-query": "^5.62.15",
    "react": "^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
